{
  "name": "social-network",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "social-network",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/social-network"
      },
      "configurations": {
        "development": {
          "outputPath": "social-network"
        },
        "production": {}
      }
    },
    "serve": {
      "executor": "@nx/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "social-network:build",
        "dev": true,
        "port": 4204
      },
      "configurations": {
        "development": {
          "buildTarget": "social-network:build:development",
          "dev": true
        },
        "production": {
          "buildTarget": "social-network:build:production",
          "dev": false
        }
      }
    },
    "export": {
      "executor": "@nx/next:export",
      "options": {
        "buildTarget": "social-network:build:production"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "social-network/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  }
}
