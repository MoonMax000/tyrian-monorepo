{
  "name": "auth-service",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/backends/auth-service",
  "projectType": "application",
  "tags": ["type:backend", "scope:auth"],
  "targets": {
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "python manage.py runserver 8001",
        "cwd": "apps/backends/auth-service"
      }
    },
    "migrate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "python manage.py migrate",
        "cwd": "apps/backends/auth-service"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pytest",
        "cwd": "apps/backends/auth-service"
      }
    },
    "docker-build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "docker build -t tyrian/auth-service -f Dockerfile .",
        "cwd": "apps/backends/auth-service"
      }
    }
  }
}

